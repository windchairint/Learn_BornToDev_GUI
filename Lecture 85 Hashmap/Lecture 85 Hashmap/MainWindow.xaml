        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="23" Margin="136,121,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="434"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="23" Margin="136,163,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="434"/>

    </Grid>
</Window>
